{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "{AppDir}\\logs\\HubIdentLog_.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Version}] ApiKeyID:{ApiKeyID} CorrelationID:{CorrelationID} {hostname} {agencia} [{Level:u3}] {Method}: {Message:lj} {NewLine}{Exception}",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30
        }
      }
    ],
    "Properties": {
      "Version": "1.0",
      "WithCorrelationId": "#{Guid.NewGuid().ToString()}"
    }
  },
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "CustomTimePrefixingFormatter",
      "FormatterOptions": {
        "CustomPrefix": "|-<[",
        "CustomSuffix": "]>-|",
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "AllowedHosts": "*",
  "API": {
    "HubUrl": "http://localhost:3000",
    "rota":"/gestaoatendimento-identificacao/v1",
    "dominio":"/cadastros",
    "ApiKey": "fe042281-51e7-4cde-983d-b5306a70d8c6-A",
    "ApiKeyIntegration": "fe042281-51e7-4cde-983d-b5306a70d8c6",
    "ClientSecret": "9b9345065cc-86fbdb68",
    "ClientId": "1e542575-7fe8-4dd8-8746-e32489798e7b"    
  }, 
  "AllowedOrigins": [
    "http://localhost:3000",
    "http://localhost:8080",
    "http://localhost:5296",
    "http://example.com"
  ]
}
