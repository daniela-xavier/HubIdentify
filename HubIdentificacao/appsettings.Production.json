{
    "Serilog": {
      "MinimumLevel": "Information",
      "WriteTo": [
        {
          "Name": "Console",
          "Args": {
            "restrictedToMinimumLevel": "Information"
          }
        },
        {
          "Name": "File",
          "Args": {
            "path": "{AppDir}\\logs\\HubIdentLog_.txt",
            "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Version}] ApiKeyID:{ApiKeyID} CorrelationID:{CorrelationID} {hostname} {agencia} [{Level:u3}] {Method}: {Message:lj} {NewLine}{Exception}",
            "rollingInterval": "Day",
            "retainedFileCountLimit": 30
          }
        }
      ],
      "Properties": {
        "Version": "1.0",
        "WithCorrelationId": "#{Guid.NewGuid().ToString()}"
      }
    },
    "DetailedErrors": true,
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "Console": {
        "FormatterName": "CustomTimePrefixingFormatter",
        "FormatterOptions": {
          "CustomPrefix": "|-<[",
          "CustomSuffix": "]>-|",
          "SingleLine": true,
          "IncludeScopes": true,
          "TimestampFormat": "HH:mm:ss ",
          "UseUtcTimestamp": true,
          "JsonWriterOptions": {
            "Indented": true
          }
        }
      }
    },
    "AllowedHosts": "*",
    "API": {
      "HubUrl": "http://localhost:5296",
      "rota":"/identifyClient",
      "dominio":"/cadastros",
      "ApiKey": "fe042281-51e7-4cde-983d-b5306a70d8c6-A",
      "ClientSecret": "123456abc",
      "ClientId": "987321654712893",    
      "issuer": "fe042281-51e7-4cde-983d-b5306a70d8c6-A",
      "iss":"https://idp.local",
      "scope":"gestao-atendimento-passante.write gestao-atendimento-passante.read"
    }
  }
  